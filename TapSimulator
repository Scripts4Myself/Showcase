local a=loadstring(game:HttpGet'https://sirius.menu/rayfield')()local b,c=(a:CreateWindow{Name='Tap Simulator',LoadingTitle='Tap Simulator Private Script',LoadingSubtitle='Made By @Rizzify101',ConfigurationSaving={Enabled=true,FolderName='TapSimHelper',FileName='Settings'},KeySystem=false})for d,e in ipairs(getgc(true))do if type(e)=='table'and type(rawget(e,'FireServer'))=='function'and type(rawget(e,'InvokeServer'))=='function'then c=e break end end if not c then local d=game:GetService'ReplicatedStorage'local e=d:FindFirstChild'Modules'if e then local f=e:FindFirstChild'Network'if f and f:IsA'ModuleScript'then pcall(function()c=require(f)end)end end end local function FireRemote(...)if c and type(c.FireServer)=='function'then pcall(c.FireServer,c,...)end end local function InvokeRemote(...)if c and type(c.InvokeServer)=='function'then pcall(c.InvokeServer,c,...)end end local d=b:CreateTab('Main',17293879614)local e,f,g=d:CreateSection'Auto Click',false,0.1 d:CreateToggle{Name='Auto Click',CurrentValue=false,Flag='AutoClickToggle',Callback=function(h)f=h end}d:CreateSlider{Name='Auto Click Speed',Range={0,2},Increment=0.01,Suffix='seconds',CurrentValue=0,Flag='ClickDelay',Callback=function(h)g=h end}task.spawn(function()while true do if f then FireRemote('Tap',true,false,false)task.wait(g)end task.wait()end end)local h=b:CreateTab('Eggs',15498677766)local i,j,k=h:CreateSection'Eggs',workspace:WaitForChild('Eggs',10),{}if j then for l,m in ipairs(j:GetChildren())do table.insert(k,m.Name)end table.sort(k)end local l,m,n,o,p={'Basic'},1,false,0.4,h:CreateLabel'Eggs Opened : N/A'task.spawn(function()local q='N/A'while true do local r=game.Players.LocalPlayer:FindFirstChild'leaderstats'if r then local s=r:FindFirstChild'Eggs'or r:FindFirstChild'Hatched'or r:FindFirstChild'EggsHatched'if s and(s:IsA'IntValue'or s:IsA'NumberValue')then local t=tostring(s.Value)if t~=q then q=t p:Set('Eggs Hatched: '..t)end end end task.wait(1.4)end end)h:CreateDropdown{Name='Select Egg',Options=#k>0 and k or{'No eggs found'},CurrentOption={'Basic'},MultipleOptions=true,Flag='EggSelection',Callback=function(q)l=q end}h:CreateDropdown{Name='Select Hatch Quantity',Options={'1','3','8'},CurrentOption={'1'},MultipleOptions=false,Flag='HatchAmount',Callback=function(q)m=tonumber(q[1])or 1 end}h:CreateSlider{Name='Hatch Delay',Range={0.1,2.5},Increment=0.05,Suffix='seconds',CurrentValue=0.1,Flag='HatchSpeedSlider',Callback=function(q)o=q end}h:CreateToggle{Name='Start Auto Hatch',CurrentValue=false,Flag='AutoHatchToggle',Callback=function(q)n=q end}task.spawn(function()while true do if n and#l>0 then for q,r in ipairs(l)do InvokeRemote('OpenEgg',r,m)end end task.wait(o)end end)local q=b:CreateTab('Player',7992557358)local r,s,t=q:CreateSection'Anti-AFK',true,game:GetService'VirtualUser'local function AntiAFKLoop()while s do pcall(function()t:CaptureController()t:ClickButton2(Vector2.new(0,0))end)task.wait(55+math.random(5,20))end end q:CreateToggle{Name='Anti-AFK',CurrentValue=true,Flag='AntiAFKToggle',Callback=function(u)s=u if u then task.spawn(AntiAFKLoop)end end}
